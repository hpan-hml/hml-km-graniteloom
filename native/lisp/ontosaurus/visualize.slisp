     "(DEFGLOBAL *VIZ-FONT-SIZE* INTEGER 7)")
    (DEFINE-STELLA-GLOBAL-VARIABLE-FROM-STRINGIFIED-SOURCE
     "(DEFGLOBAL VIZ-TOP STRING \"top\")")
    (DEFINE-STELLA-GLOBAL-VARIABLE-FROM-STRINGIFIED-SOURCE
