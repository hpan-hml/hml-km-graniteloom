    (HTML-DESCRIBE-ONE-SUPER-LEVEL DIRECTSUPERS ALREADYOUTPUT STREAM))
   (CL:WHEN RULE? (%%PRINT-STREAM STREAM "<HR SIZE=2>" EOL)) NULL))

