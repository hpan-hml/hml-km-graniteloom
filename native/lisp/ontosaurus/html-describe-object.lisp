   (CL:WHEN (CL:EQ MODULE NULL)
    (HTML-UNKNOWN-MODULE-RESPONSE KWD-HTML-DESCRIBE-OBJECT-RELATION
     OBJECTNAME MODULENAME STREAM)
    (CL:RETURN-FROM HTML-WRITE-OBJECT-RESPONSE-PAGE))
   (CL:LET* ((*MODULE* MODULE) (*CONTEXT* *MODULE*))
    (CL:DECLARE (CL:SPECIAL *MODULE* *CONTEXT*))
    (CL:LET*
     ((INSTANCE
       (CL:IF (STARTS-WITH? OBJECTNAME "(" 0)
        (COERCE-TO-INSTANCE (READ-S-EXPRESSION-FROM-STRING OBJECTNAME)
         NULL)
        (PLI::GET-OBJECT OBJECTNAME NULL NULL))))
     (CL:IF (CL:NOT (CL:EQ INSTANCE NULL))
      (CL:LET* ((TEST-VALUE-000 (SAFE-PRIMARY-TYPE INSTANCE)))
       (CL:COND
        ((SUBTYPE-OF? TEST-VALUE-000
          SGT-HTML-DESCRIBE-OBJECT-LOGIC-NAMED-DESCRIPTION)
         (CL:PROGN
          (HTML-WRITE-RELATION-RESPONSE-PAGE INSTANCE STREAM)))
