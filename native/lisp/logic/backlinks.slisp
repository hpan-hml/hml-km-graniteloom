    (CL:SETQ DESCRIPTION (%ITERATOR.VALUE ITER-000))
    (CL:WHEN
     (CL:AND
      (CL:NOT (CL:EQ (%NAMED-DESCRIPTION.EXTENSION DESCRIPTION) NULL))
      (CL:NOT
