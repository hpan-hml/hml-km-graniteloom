  (CL:LET*
   ((MODULE (SAFELY-GET-MODULE MODULE-NAME ENVIRONMENT))
    (RELATION (SAFELY-GET-RELATION RELATION-NAME MODULE ENVIRONMENT)))
   (CL:IF (CL:NOT (CL:EQ RELATION STELLA::NULL)) (GET-ARITY RELATION)
    STELLA::NULL-INTEGER)))

