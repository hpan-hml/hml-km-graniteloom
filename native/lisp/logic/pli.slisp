  (STELLA::%%PRINT-STREAM
   (STELLA::%OUTPUT-STREAM.NATIVE-STREAM STELLA::STANDARD-OUTPUT)
   "Initializing STELLA..." EOL)
  (STELLA::STARTUP-STELLA-SYSTEM)
