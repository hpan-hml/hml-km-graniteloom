     (CL:LET* ((SELF-000 (STELLA::NEW-INPUT-STREAM)))
      (CL:SETF (STELLA::%INPUT-STREAM.NATIVE-STREAM SELF-000) STREAM)
      (STELLA::LOAD-STREAM SELF-000))))))

