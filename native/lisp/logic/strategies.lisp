   (CL:WHEN (CL:EQ PMF NULL)
    (CREATE-AND-LINK-PARTIAL-MATCH-FRAME SELF KWD-STRATEGIES-FAIL))
   (CONTINUE-PARTIAL-FAIL-PROOF (%PARTIAL-MATCH-FRAME SELF) LASTMOVE)))

(CL:DEFUN HELP-STARTUP-STRATEGIES1 ()
