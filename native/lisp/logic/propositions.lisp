   (CL:RETURN-FROM LOG-LOGIC-OBJECT))
  (CL:WHEN (CL:NOT *LOADINGREGENERABLEOBJECTS?*)
   (PUSH (LOCALLY-CONCEIVED-INSTANCES *CONTEXT*) SELF)))

