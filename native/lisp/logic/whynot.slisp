(CL:DEFUN NEW-WHYNOT-PARTIAL-MATCH (KIND CONTROL-FRAME)
  (CL:LET* ((SELF NULL)) (CL:SETQ SELF (%MAKE-WHYNOT-PARTIAL-MATCH))
   (CL:SETF (%WHYNOT-PARTIAL-MATCH.KIND SELF) KIND)
   (CL:SETF (%WHYNOT-PARTIAL-MATCH.CONTROL-FRAME SELF) CONTROL-FRAME)
   (CL:SETF (%WHYNOT-PARTIAL-MATCH.SUCCESS? SELF) CL:NIL)
   (CL:SETF (%WHYNOT-PARTIAL-MATCH.UNBOUND-VARS SELF) NIL)
   (CL:SETF (%WHYNOT-PARTIAL-MATCH.ARGUMENT-WEIGHTS SELF) NIL)
   (CL:SETF (%WHYNOT-PARTIAL-MATCH.ARGUMENT-SCORES SELF) NIL)
   (CL:SETF (%WHYNOT-PARTIAL-MATCH.DYNAMIC-CUTOFF SELF) NULL-FLOAT)
   (CL:SETF (%WHYNOT-PARTIAL-MATCH.NEGATIVE-SCORE SELF) NULL-FLOAT)
   (CL:SETF (%WHYNOT-PARTIAL-MATCH.POSITIVE-SCORE SELF) NULL-FLOAT)
   (CL:SETF (%WHYNOT-PARTIAL-MATCH.MAXIMUM-SCORE SELF) 0.0d0)
   (CL:SETF (%WHYNOT-PARTIAL-MATCH.TOTAL-WEIGHT SELF) NULL-FLOAT)
   (CL:SETF (%WHYNOT-PARTIAL-MATCH.ACCUMULATED-WEIGHT SELF) 0.0d0)
   (CL:SETF (%WHYNOT-PARTIAL-MATCH.ACCUMULATED-SCORE SELF) 0.0d0)
   (CL:SETF (%WHYNOT-PARTIAL-MATCH.CHILD SELF) NULL)
   (CL:SETF (%WHYNOT-PARTIAL-MATCH.PARENT SELF) NULL)
   (CL:SETF (%WHYNOT-PARTIAL-MATCH.UNKNOWN-GOAL-CUTOFF SELF) NULL-INTEGER)
   (INITIALIZE-INCREMENTAL-PARTIAL-MATCH SELF) SELF))

(CL:DEFMETHOD PRIMARY-TYPE ((SELF WHYNOT-PARTIAL-MATCH))
  SGT-WHYNOT-LOGIC-WHYNOT-PARTIAL-MATCH)

(CL:DEFUN ACCESS-WHYNOT-PARTIAL-MATCH-SLOT-VALUE (SELF SLOTNAME VALUE SETVALUE?)
  (CL:COND
   ((CL:EQ SLOTNAME SYM-WHYNOT-LOGIC-UNKNOWN-GOAL-CUTOFF)
    (CL:IF SETVALUE?
     (CL:SETF (%WHYNOT-PARTIAL-MATCH.UNKNOWN-GOAL-CUTOFF SELF)
      (%INTEGER-WRAPPER.WRAPPER-VALUE VALUE))
     (CL:SETQ VALUE
      (WRAP-INTEGER (%WHYNOT-PARTIAL-MATCH.UNKNOWN-GOAL-CUTOFF SELF)))))
   (CL:T
    (CL:LET* ((STREAM-000 (NEW-OUTPUT-STRING-STREAM)))
     (%%PRINT-STREAM (%OUTPUT-STRING-STREAM.NATIVE-STREAM STREAM-000) "`"
      SLOTNAME "' is not a valid case option")
     (CL:ERROR (NEW-STELLA-EXCEPTION (THE-STRING-READER STREAM-000))))))
  VALUE)

