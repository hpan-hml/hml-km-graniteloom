;;; (DEFGLOBAL SUBSUMPTION-INFERENCE ...)

(CL:DEFVAR SUBSUMPTION-INFERENCE NULL)

;;; (DEFGLOBAL SHALLOW-INFERENCE ...)

(CL:DEFVAR SHALLOW-INFERENCE NULL)

