   (MEMBER? *CURRENT-POWERLOOM-FEATURES* KWD-QUERY-TRACE-SUBGOALS)
   (CL:PROGN (%UNSET-FEATURE (CONS-LIST KWD-QUERY-TRACE-SUBGOALS))
