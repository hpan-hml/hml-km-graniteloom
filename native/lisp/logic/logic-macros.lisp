      (LIST* SYM-LOGIC-MACROS-STELLA-SETF
       (LIST* SYM-LOGIC-MACROS-LOGIC-ALLOTTED-CLOCK-TICKS
        SYM-LOGIC-MACROS-LOGIC-PARENT NIL)
       (LIST* SYM-LOGIC-MACROS-STELLA--
        (LIST* SYM-LOGIC-MACROS-LOGIC-ALLOTTED-CLOCK-TICKS
         SYM-LOGIC-MACROS-LOGIC-PARENT NIL)
        (LIST* SYM-LOGIC-MACROS-STELLA--
