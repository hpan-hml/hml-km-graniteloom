    (CONS SYM-LOGIC-MACROS-STELLA-CONS-LIST
     (CONCATENATE (NTH-REST BODY 3) NIL))
    (LIST* SYM-LOGIC-MACROS-STELLA-QUOTE GENSYMNAME NIL) NIL)))

