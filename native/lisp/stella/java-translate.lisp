   ((DIMENSIONS (ARRAY-TYPE-DIMENSIONS ARRAYTYPE))
    (ELEMENTTYPE
     (EXTRACT-PARAMETER-TYPE ARRAYTYPE SYM-JAVA-TRANSLATE-STELLA-ANY-VALUE))
    (TRANSLATEDTYPE (%WRAPPER-VALUE (JAVA-TRANSLATE-TYPE-SPEC ELEMENTTYPE))))
   (CL:DECLARE (CL:TYPE CL:SIMPLE-STRING TRANSLATEDTYPE))
   (CL:CASE (LENGTH DIMENSIONS)
    (1
     (CL:RETURN-FROM JAVA-TRANSLATE-ARRAY-TYPE
      (WRAP-STRING (CONCATENATE TRANSLATEDTYPE "[]"))))
    (2
     (CL:RETURN-FROM JAVA-TRANSLATE-ARRAY-TYPE
      (WRAP-STRING (CONCATENATE TRANSLATEDTYPE "[][]"))))
    (3
     (CL:RETURN-FROM JAVA-TRANSLATE-ARRAY-TYPE
      (WRAP-STRING (CONCATENATE TRANSLATEDTYPE "[][][]"))))
    (CL:OTHERWISE
     (CL:LET* ((UNUSED-DIM NULL) (ITER-000 (%THE-CONS-LIST DIMENSIONS)))
      (CL:LOOP WHILE (CL:NOT (CL:EQ ITER-000 NIL)) DO
       (CL:SETQ UNUSED-DIM (%%VALUE ITER-000)) (CL:SETQ UNUSED-DIM UNUSED-DIM)
       (CL:SETQ TRANSLATEDTYPE (CONCATENATE TRANSLATEDTYPE "[]"))
       (CL:SETQ ITER-000 (%%REST ITER-000))))
     (CL:RETURN-FROM JAVA-TRANSLATE-ARRAY-TYPE (WRAP-STRING TRANSLATEDTYPE))))))

