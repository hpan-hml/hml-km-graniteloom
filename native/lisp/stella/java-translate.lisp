     (CL:RETURN-FROM JAVA-TRANSLATE-DEFINED-OR-NULL
      (JAVA-TRANSLATE-OPERATOR-CALL (CONS (WRAP-STRING OPERATOR) NIL)
       (CONS OBJECT (CONS SYM-JAVA-TRANSLATE-STELLA-NULL NIL)) 2)))
    ((SUBTYPE-OF? CLASSTYPE SGT-JAVA-TRANSLATE-STELLA-INTEGER)
     (CL:RETURN-FROM JAVA-TRANSLATE-DEFINED-OR-NULL
      (JAVA-TRANSLATE-OPERATOR-CALL (CONS (WRAP-STRING OPERATOR) NIL)
       (CONS OBJECT (CONS SYM-JAVA-TRANSLATE-STELLA-NULL-INTEGER NIL)) 2)))
    ((SUBTYPE-OF? CLASSTYPE SGT-JAVA-TRANSLATE-STELLA-FLOAT)
     (CL:RETURN-FROM JAVA-TRANSLATE-DEFINED-OR-NULL
      (JAVA-TRANSLATE-OPERATOR-CALL (CONS (WRAP-STRING OPERATOR) NIL)
       (CONS OBJECT (CONS SYM-JAVA-TRANSLATE-STELLA-NULL-FLOAT NIL)) 2)))
    ((SUBTYPE-OF? CLASSTYPE SGT-JAVA-TRANSLATE-STELLA-SINGLE-FLOAT)
     (CL:RETURN-FROM JAVA-TRANSLATE-DEFINED-OR-NULL
