   (CL:RETURN-FROM JAVA-TRANSLATE-GLOBAL-NAME (WRAP-STRING "NULL")))
  (CL:WHEN (CL:NOT DEFINITION?)
   (CL:WHEN (CL:EQ NAMESYMBOL SYM-JAVA-TRANSLATE-STELLA-TRUE)
