     (CL:VALUES
      (WRAP-STRING
       (CONCATENATE (JAVA-PACKAGE-PREFIX (HOME-MODULE F) ".")
        (%WRAPPER-VALUE
         (JAVA-TRANSLATE-CLASS-NAMESTRING
          (WRAP-STRING
           (%WRAPPER-VALUE
            (DYNAMIC-SLOT-VALUE (%DYNAMIC-SLOTS F)
             SYM-JAVA-TRANSLATE-STELLA-METHOD-STARTUP-CLASSNAME
             NULL-STRING-WRAPPER)))))))
      (JAVA-TRANSLATE-FUNCTION-NAME F))))
   ((%WRAPPER-VALUE
     (DYNAMIC-SLOT-VALUE (%DYNAMIC-SLOTS F)
