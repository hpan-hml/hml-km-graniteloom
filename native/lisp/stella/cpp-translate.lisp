;;; (DEFUN (OVERLOADED-FUNCTION-NAME-MODULE MODULE) ...)

(CL:DEFUN OVERLOADED-FUNCTION-NAME-MODULE (FUNCTION-NAME CLASS-TYPE)
  (CL:LET*
   ((SYMBOL-MODULE (HOME-MODULE FUNCTION-NAME))
    (CLASS-MODULE (HOME-MODULE CLASS-TYPE)))
   (CL:COND
    ((CL:EQ SYMBOL-MODULE CLASS-MODULE)
     (CL:RETURN-FROM OVERLOADED-FUNCTION-NAME-MODULE SYMBOL-MODULE))
    ((MEMBER? (%PARENT-MODULES CLASS-MODULE) SYMBOL-MODULE)
     (CL:RETURN-FROM OVERLOADED-FUNCTION-NAME-MODULE CLASS-MODULE))
    ((MEMBER? (%PARENT-MODULES SYMBOL-MODULE) CLASS-MODULE)
     (CL:RETURN-FROM OVERLOADED-FUNCTION-NAME-MODULE SYMBOL-MODULE))
    (CL:T
     (CL:RETURN-FROM OVERLOADED-FUNCTION-NAME-MODULE CLASS-MODULE)))))

