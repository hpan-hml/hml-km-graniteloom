(CL:DEFUN CPP-NON-POINTER-TYPE? (TYPESPEC)
  (CL:LET* ((BASETYPE (TYPE-SPEC-TO-BASE-TYPE TYPESPEC)))
   (CL:COND
    ((CL:OR (CL:EQ BASETYPE SGT-CPP-TRANSLATE-STELLA-INTEGER)
      (CL:EQ BASETYPE SGT-CPP-TRANSLATE-STELLA-SHORT-INTEGER)
      (CL:EQ BASETYPE SGT-CPP-TRANSLATE-STELLA-LONG-INTEGER)
      (CL:EQ BASETYPE SGT-CPP-TRANSLATE-STELLA-UNSIGNED-LONG-INTEGER)
      (CL:EQ BASETYPE SGT-CPP-TRANSLATE-STELLA-FLOAT)
      (CL:EQ BASETYPE SGT-CPP-TRANSLATE-STELLA-SINGLE-FLOAT)
      (CL:EQ BASETYPE SGT-CPP-TRANSLATE-STELLA-DOUBLE-FLOAT)
      (CL:EQ BASETYPE SGT-CPP-TRANSLATE-STELLA-CHARACTER)
      (CL:EQ BASETYPE SGT-CPP-TRANSLATE-STELLA-BYTE)
      (CL:EQ BASETYPE SGT-CPP-TRANSLATE-STELLA-OCTET))
     (CL:RETURN-FROM CPP-NON-POINTER-TYPE? CL:T))
    (CL:T (CL:RETURN-FROM CPP-NON-POINTER-TYPE? CL:NIL)))))

;;; (DEFUN (CPP-TYPE-WITHOUT-INTERIOR-POINTERS? BOOLEAN) ...)

(CL:DEFUN CPP-TYPE-WITHOUT-INTERIOR-POINTERS? (TYPESPEC)
  (CL:LET*
   ((BASETYPE (TYPE-SPEC-TO-BASE-TYPE TYPESPEC))
    (CLASS (%SURROGATE-VALUE BASETYPE)))
   (CL:WHEN (ARRAY-TYPE-SPECIFIER? TYPESPEC)
    (CL:RETURN-FROM CPP-TYPE-WITHOUT-INTERIOR-POINTERS?
     (CPP-NON-POINTER-TYPE?
      (EXTRACT-PARAMETER-TYPE TYPESPEC
       SYM-CPP-TRANSLATE-STELLA-ANY-VALUE))))
   (CL:WHEN
    (CL:AND (CREATE-NATIVE-CLASS? CLASS)
     (CL:NOT
      (SUBTYPE-OF? BASETYPE
       SGT-CPP-TRANSLATE-STELLA-NATIVE-EXCEPTION)))
    (CL:LET* ((ALWAYS?-000 CL:T))
     (CL:LET* ((SLOT NULL) (ITER-000 (CLASS-SLOTS CLASS)))
      (CL:LOOP WHILE (NEXT? ITER-000) DO
       (CL:SETQ SLOT (%VALUE ITER-000))
       (CL:WHEN
        (CL:AND (STORAGE-SLOT? SLOT) (NATIVE-SLOT? SLOT)
         (CL:NOT (SLOT-HAS-UNKNOWN-TYPE? SLOT CLASS)))
        (CL:WHEN
         (CL:NOT (CPP-NON-POINTER-TYPE? (%SLOT-BASE-TYPE SLOT)))
         (CL:SETQ ALWAYS?-000 CL:NIL) (CL:RETURN)))))
     (CL:LET* ((VALUE-000 ALWAYS?-000))
      (CL:RETURN-FROM CPP-TYPE-WITHOUT-INTERIOR-POINTERS? VALUE-000))))
   (CL:RETURN-FROM CPP-TYPE-WITHOUT-INTERIOR-POINTERS? CL:NIL)))

