;;; (DEFUN (CPP-TRANSLATE-HANDLE-EXCEPTION CONS) ...)

(CL:DEFUN CPP-TRANSLATE-HANDLE-EXCEPTION (TREE)
  (CL:LET*
   ((VARIABLEDECL (%%VALUE (%%REST TREE)))
    (VARIABLE (%%VALUE VARIABLEDECL))
    (EXCEPTIONTYPE (%%VALUE (%%REST VARIABLEDECL)))
    (HELPERVARIABLE NULL) (HELPERVARIABLETYPE NULL))
   (FIRST-SETTER TREE SYM-CPP-TRANSLATE-STELLA-CPP_CATCH)
   (CL:WHEN (CL:NOT (CL:EQ VARIABLE NULL))
    (CL:SETQ HELPERVARIABLE
     (WRAP-STRING
      (CONCATENATE "_"
       (%WRAPPER-VALUE (CPP-TRANSLATE-NAME VARIABLE)))))
    (CL:SETQ HELPERVARIABLETYPE
     (WRAP-STRING
      (STRING-CONCATENATE
       (%WRAPPER-VALUE (CPP-TRANSLATE-TYPE-SPEC EXCEPTIONTYPE)) "&"))))
   (SECOND-SETTER TREE
    (CONS
     (LIST* SYM-CPP-TRANSLATE-STELLA-CPP_TYPE
      (CL:IF (CL:NOT (CL:EQ VARIABLE NULL)) HELPERVARIABLETYPE
       (CPP-TRANSLATE-TYPE-SPEC EXCEPTIONTYPE))
      NIL)
     (CONCATENATE
      (CL:IF (CL:NOT (CL:EQ VARIABLE NULL))
       (CONS
        (LIST* SYM-CPP-TRANSLATE-STELLA-CPP_IDENT HELPERVARIABLE NIL)
        NIL)
       NIL)
      NIL)))
   (CL:SETF (%%REST (%%REST TREE))
    (CPP-TRANSLATE-LIST-OF-TREES (%%REST (%%REST TREE))))
   (CL:WHEN (CL:NOT (CL:EQ VARIABLE NULL))
    (CL:SETF (%%REST (%%REST TREE))
     (CONS
      (LIST* SYM-CPP-TRANSLATE-STELLA-CPP_LOCAL NULL
       (CONS (CPP-TRANSLATE-AND-POINTERIZE-TYPE-SPEC EXCEPTIONTYPE)
        (LIST*
         (LIST* SYM-CPP-TRANSLATE-STELLA-CPP_IDENT
          (CPP-TRANSLATE-NAME VARIABLE) NIL)
         (LIST* SYM-CPP-TRANSLATE-STELLA-CPP_IDENT
          (WRAP-STRING
           (STRING-CONCATENATE "&" (%WRAPPER-VALUE HELPERVARIABLE)))
          NIL)
         NIL)))
      (CONCATENATE (%%REST (%%REST TREE)) NIL))))
   (CL:RETURN-FROM CPP-TRANSLATE-HANDLE-EXCEPTION TREE)))

