   *CPP-NAME-QUALIFICATION-MODE*))

;;; (DEFUN (CPP-QUALIFY-NAME? BOOLEAN) ...)

(CL:DEFUN CPP-QUALIFY-NAME? (NAMESYMBOL)
  (CL:LET*
   ((SYMBOLMODULE (%HOME-CONTEXT NAMESYMBOL))
    (NAMESPACEMODULE
     (CL:IF (CL:NOT (CL:EQ *CPP-CURRENT-UNIT-MODULE* NULL))
      *CPP-CURRENT-UNIT-MODULE* *MODULE*)))
   (CL:WHEN
    (CL:OR (CL:EQ SYMBOLMODULE NULL)
     (CL:EQ NAMESYMBOL SYM-CPP-TRANSLATE-STELLA-NULL))
    (CL:RETURN-FROM CPP-QUALIFY-NAME? CL:NIL))
   (CL:WHEN
    (CL:AND (SYMBOL? NAMESYMBOL)
     (NATIVE-NAME? NAMESYMBOL KWD-CPP-TRANSLATE-CPP NULL))
    (CL:RETURN-FROM CPP-QUALIFY-NAME? CL:T))
   (CL:COND
    ((CL:EQ *CPP-NAME-QUALIFICATION-MODE* KWD-CPP-TRANSLATE-FULL)
     (CL:RETURN-FROM CPP-QUALIFY-NAME? CL:T))
    ((CL:EQ *CPP-NAME-QUALIFICATION-MODE* KWD-CPP-TRANSLATE-NEVER)
     (CL:RETURN-FROM CPP-QUALIFY-NAME? CL:NIL))
    ((CL:EQ *CPP-NAME-QUALIFICATION-MODE* KWD-CPP-TRANSLATE-CLEVER)
     (CL:COND
      ((CL:EQ SYMBOLMODULE NAMESPACEMODULE)
       (CL:RETURN-FROM CPP-QUALIFY-NAME? CL:NIL))
      ((CL:NOT (VISIBLE-FROM? SYMBOLMODULE NAMESPACEMODULE))
       (CL:RETURN-FROM CPP-QUALIFY-NAME? CL:T))
      (CL:T
       (CL:LET* ((TEST-VALUE-000 (SAFE-PRIMARY-TYPE NAMESYMBOL)))
        (CL:COND
         ((SUBTYPE-OF-SYMBOL? TEST-VALUE-000)
          (CL:PROGN
           (CL:RETURN-FROM CPP-QUALIFY-NAME?
            (CL:NOT
             (CL:EQ
              (%%REST
               (LOOKUP-VISIBLE-SYMBOLS-IN-MODULE
                (%SYMBOL-NAME NAMESYMBOL) NAMESPACEMODULE CL:NIL))
              NIL)))))
         ((SUBTYPE-OF-SURROGATE? TEST-VALUE-000)
          (CL:PROGN
           (CL:RETURN-FROM CPP-QUALIFY-NAME?
            (CL:NOT
             (CL:EQ
              (%%REST
               (LOOKUP-VISIBLE-SURROGATES-IN-MODULE
                (%SYMBOL-NAME NAMESYMBOL) NAMESPACEMODULE CL:NIL))
              NIL)))))
         (CL:T
          (CL:LET* ((STREAM-000 (NEW-OUTPUT-STRING-STREAM)))
           (%%PRINT-STREAM (%NATIVE-STREAM STREAM-000) "`"
            TEST-VALUE-000 "' is not a valid case option")
           (CL:ERROR
            (NEW-STELLA-EXCEPTION
             (THE-STRING-READER STREAM-000))))))))))
    (CL:T
     (CL:LET* ((STREAM-001 (NEW-OUTPUT-STRING-STREAM)))
      (%%PRINT-STREAM (%NATIVE-STREAM STREAM-001) "`"
       *CPP-NAME-QUALIFICATION-MODE* "' is not a valid case option")
      (CL:ERROR
       (NEW-STELLA-EXCEPTION (THE-STRING-READER STREAM-001))))))))

