   (CL:WHEN (CL:NOT (CL:EQ CLASS NULL))
    (CL:WHEN (CL:NOT (CL:EQ *CONSTRUCTOR-DEMONS* NULL))
     (CL:LET* ((DEMON NULL) (ITER-000 (%THE-CONS-LIST *CONSTRUCTOR-DEMONS*)))
