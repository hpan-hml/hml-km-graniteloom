       (CL:SETQ ITER-000 (%%REST ITER-000)))))
    (CL:WHEN (CL:NOT (CL:EQ (CLASS-CONSTRUCTOR-DEMONS CLASS) NULL))
     (CL:LET*
      ((DEMON NULL)
       (ITER-001 (%THE-CONS-LIST (CLASS-CONSTRUCTOR-DEMONS CLASS))))
      (CL:LOOP WHILE (CL:NOT (CL:EQ ITER-001 NIL)) DO
