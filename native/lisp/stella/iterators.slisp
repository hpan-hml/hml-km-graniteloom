    (CONS (WRAP-FUNCTION-CODE (CL:FUNCTION FILTER-PRIVATE-SLOT?))
     (CONS (WRAP-FUNCTION-CODE (CL:FUNCTION FILTER-NON-EXTERNAL-SLOT?))
      (CONS
       (WRAP-FUNCTION-CODE (CL:FUNCTION FILTER-NON-AUXILIARY-SLOT?))
       NIL))))))

