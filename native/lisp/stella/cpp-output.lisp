        ((CL:EQ TEST-VALUE-000 SYM-CPP-OUTPUT-STELLA-CPP_PROGN)
         (CPP-OUTPUT-PROGN (%%VALUE (%%REST STATEMENT))))
        ((CL:EQ TEST-VALUE-000
          SYM-CPP-OUTPUT-STELLA-CPP_WITH_PROCESS_LOCK)
