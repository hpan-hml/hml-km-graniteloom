  (CL:WHEN (CL:NOT (CL:EQ PARAMETERS NIL))
   (%%PRINT-STREAM (%NATIVE-STREAM *CURRENT-STREAM*) "template <")
   (%%PRINT-STREAM (%NATIVE-STREAM *CURRENT-STREAM*) "class "
    (%WRAPPER-VALUE (%%VALUE PARAMETERS)))
   (CL:LET* ((PARAMETER NULL) (ITER-000 (%%REST PARAMETERS)))
    (CL:LOOP WHILE (CL:NOT (CL:EQ ITER-000 NIL)) DO
     (CL:SETQ PARAMETER (%%VALUE ITER-000))
     (%%PRINT-STREAM (%NATIVE-STREAM *CURRENT-STREAM*) "class "
      PARAMETER ", ")
     (CL:SETQ ITER-000 (%%REST ITER-000))))
   (%%PRINT-STREAM (%NATIVE-STREAM *CURRENT-STREAM*) ">" EOL))
