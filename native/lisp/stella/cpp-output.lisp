   (%%PRINT-STREAM (%NATIVE-STREAM *CURRENT-STREAM*) " {" EOL)
   (CPP-BUMP-INDENT) (CPP-OUTPUT-STATEMENT BODY) (CPP-UNBUMP-INDENT)
   (%%PRINT-STREAM (%NATIVE-STREAM *CURRENT-STREAM*) "}" EOL)
   (%%PRINT-STREAM (%NATIVE-STREAM *CURRENT-STREAM*) EOL))
  :VOID)

