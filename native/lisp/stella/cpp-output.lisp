    (CL:PROGN
     (CL:RETURN-FROM CPP-ARGUMENT-IS-STREAM?
      (CL:AND (CL:EQ (%%VALUE ARG) SYM-CPP-OUTPUT-STELLA-CPP_IDENT)
       (EQL-TO-STRING? (%%VALUE (%%REST ARG)) "stream")))))
   (CL:T (CL:RETURN-FROM CPP-ARGUMENT-IS-STREAM? CL:NIL))))

