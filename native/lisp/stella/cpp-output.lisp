   (%%PRINT-STREAM (%NATIVE-STREAM *CURRENT-STREAM*)
    (%WRAPPER-VALUE (%%VALUE CLASSLIST))))
  (CL:LET* ((CLASS NULL) (ITER-000 (%%REST CLASSLIST)))
   (CL:LOOP WHILE (CL:NOT (CL:EQ ITER-000 NIL)) DO
    (CL:SETQ CLASS (%%VALUE ITER-000))
    (%%PRINT-STREAM (%NATIVE-STREAM *CURRENT-STREAM*) ", public ")
    (CL:COND
     ((CL:EQ *TRANSLATOROUTPUTLANGUAGE* KWD-CPP-OUTPUT-CPP-STANDALONE)
      (%%PRINT-STREAM (%NATIVE-STREAM *CURRENT-STREAM*) "virtual "))
     (CL:T))
    (%%PRINT-STREAM (%NATIVE-STREAM *CURRENT-STREAM*)
     (%WRAPPER-VALUE CLASS))
    (CL:SETQ ITER-000 (%%REST ITER-000))))
  (%%PRINT-STREAM (%NATIVE-STREAM *CURRENT-STREAM*) " ")
